@model IEnumerable<AppUserModel>
@{
    ViewData["Title"] = "List User";

}
<h3>List Category</h3>
<a asp-action="Create" asp-controller="User">Add User</a>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>UserName</th>
            <th>Email</th>
            <th>PasswordHash</th>
            <th>Phone</th>
            <th>Role</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
                <td>@user.Email</td>
                <td>@user.PasswordHash</td>
                <td>@user.PhoneNumber</td>
                <td></td>

                <td>
                    <a asp-action="Edit" class="btn btn-warning btn-sm" asp-controller="User" asp-route-Id="@user.Id">Edit User</a>
                    <a asp-action="Delete" class="btn btn-danger btn-sm confirmDeletion" asp-controller="User" asp-route-Id="@user.Id">Delete User</a>
                </td>
            </tr>
        }

    </tbody>
</table>